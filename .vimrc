set nocompatible
syntax on
filetype plugin indent on
let g:loaded_matchparen = 1

set number
set relativenumber
set laststatus=2
set showcmd

set encoding=utf-8
set tabstop=4
set shiftwidth=4
set expandtab
set autoindent
"set smartindent
set expandtab
set backspace=indent,eol,start

set hlsearch
set incsearch
set ignorecase

set background=dark
colorscheme  slate
